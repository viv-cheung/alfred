{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2020",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "src",
      "paths": {
        "src/*": ["*"]
      }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  